<h3>User List</h3>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Phone</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.age}}</td>
            <td>{{this.phone}}</td>
            <td>{{this.email}}</td>
            <td>
                <a href="/user/update/{{this._id}}">Edit</a>
                <a href="/user/delete/{{this._id}}" onclick="return confirm('Are you sure you want to delete this user?');">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<a href="/user">Create New</a>